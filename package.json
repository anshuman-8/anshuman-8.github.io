{
  "name": "answain.com",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:export": "next build",
    "build:workers": "opennextjs-cloudflare build",
    "clean": "rm -rf .next out .open-next",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "preview:workers": "opennextjs-cloudflare build && wrangler dev",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:pages": "npm run build:export && wrangler pages deploy out --project-name=answain-com",
    "deploy:preview": "npm run build:export && wrangler pages deploy out --project-name=answain-com --env preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "pages:dev": "wrangler pages dev out",
    "pages:deploy": "wrangler pages deploy out",
    "workers:dev": "wrangler dev",
    "workers:deploy": "npm run build:workers && wrangler deploy",
    "build:clean": "rm -rf .next out .open-next && npm run build",
    "wrangler:login": "wrangler login",
    "wrangler:whoami": "wrangler whoami"
  },
  "dependencies": {
    "@bradgarropy/next-seo": "^1.2.0",
    "@opennextjs/cloudflare": "^1.11.0",
    "@types/prismjs": "^1.26.5",
    "@vercel/analytics": "^1.0.2",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "next": "^14.2.0",
    "prismjs": "^1.30.0",
    "react": "^18.3.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.0",
    "react-icons": "^4.12.0",
    "react-spinners": "^0.13.8",
    "react-syntax-highlighter": "^15.5.0",
    "react-top-loading-bar": "^2.3.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "wrangler": "^4.38.0"
  }
}
